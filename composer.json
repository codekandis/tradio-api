{
	"name": "codekandis/tradio-api",
	"description": "This is an API for web radio applications.",
	"keywords": [
		"php",
		"tradio",
		"web",
		"radio",
		"applications"
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/tradio-api/issues",
		"docs": "https://github.com/codekandis/tradio-api/blob/master/README.md",
		"source": "https://github.com/codekandis/tradio-api/tree/master"
	},
	"require": {
		"php": "^7.3",
		"codekandis/tiphy": "^0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"sensiolabs/security-checker": "^6",
		"phpunit/phpunit": "^8"
	},
	"repositories": {
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\TradioApi\\": [
				"config/",
				"public/",
				"src/",
				"templates/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\TradioApi\\Tests\\": [
				"tests/"
			]
		}
	},
	"scripts": {
		"post-update-cmd": [
			"[ ${COMPOSER_DEV_MODE} -eq 0 ] || vendor/bin/security-checker security:check composer.lock"
		]
	}
}
